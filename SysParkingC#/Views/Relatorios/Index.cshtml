@model IEnumerable<SysParkingC_.Models.Relatorio>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TotalVeiculosEstacionados)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TotalArrecadado)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TempoTotalPermanenciaHoras)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PeriodoInicio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PeriodoFim)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TotalVeiculosEstacionados)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TotalArrecadado)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TempoTotalPermanenciaHoras)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PeriodoInicio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PeriodoFim)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
